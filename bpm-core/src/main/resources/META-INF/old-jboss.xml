<?xml version="1.0" encoding="UTF-8"?>
<jboss>
	<security-domain>java:/jaas/seycon</security-domain>


	<enterprise-beans>
		<session>
			<ejb-name>ejb/BPMEngineBeanLocal</ejb-name>
			<local-jndi-name>java:local/ejb/BPMEngineBeanLocal</local-jndi-name>
		</session>

		<session>
			<ejb-name>ejb/BPMJobExecutorBeanLocal</ejb-name>
			<local-jndi-name>java:local/ejb/BPMJobExecutorBeanLocal</local-jndi-name>
		</session>

		<session>
			<ejb-name>ejb/BPMConfigLocal</ejb-name>
			<local-jndi-name>java:local/ejb/BPMConfigLocal</local-jndi-name>
		</session>

	</enterprise-beans>

	<unauthenticated-principal>nobody</unauthenticated-principal>
</jboss>