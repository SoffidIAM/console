<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	 <class name="es.caib.bpm.entity.ProcessFile" table="BPM_PROCESS_FILE">
        
        <id name="id">
            <column name="PRO_ID" />
            <generator class="native">
            </generator>
        </id>
		
		<property name="instanceId" column="PRO_INSTANCE_ID" type="long" not-null="true"/>
		<property name="documentId" column="PRO_DOC_ID" type="long" not-null="true"/>
		<property name="hashCode" column="PRO_DOC_HASH" type="string" not-null="true"/>
		<property name="deleted" column="PRO_DELETED" type="boolean" not-null="true"/>
    </class>
</hibernate-mapping>
