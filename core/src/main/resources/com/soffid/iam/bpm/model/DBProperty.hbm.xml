<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	 <class name="com.soffid.iam.bpm.model.DBProperty" table="BPM_DATABASE_PROPERTY">
        
        <id name="id">
            <column name="DBP_ID" />
            <generator class="native">
            </generator>
        </id>
		
		<property name="app" column="DBP_APP" not-null="true"/>
		<property name="key" column="DBP_KEY" not-null="true"/>
		<property name="value" column="DBP_VALUE" not-null="true"/>
    </class>
</hibernate-mapping>
