<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	 <class name="es.caib.bpm.nas.entity.BpmAccessLog" table="BPM_ACCESS_LOG">
        
        <id name="id">
            <column name="ACC_ID" />
            <generator class="native">
            </generator>
        </id>
		
		<property name="type" column="ACC_TYPE" type="character" not-null="true"/>
	
		<many-to-one name="bpmDocument" class="es.caib.bpm.nas.entity.BpmDocument" fetch="select">
			<column name="ACC_DOC_ID" not-null="true"/>
		</many-to-one>  
		<property name="user" column="ACC_USER" type="string" not-null="true"/>
		
		<property name="date" column="ACC_DATE" type="timestamp" not-null="true"/>
			
    </class>
</hibernate-mapping>
