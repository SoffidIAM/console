<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	 <class name="es.caib.bpm.nas.entity.BpmFileSystem" table="BPM_FILE_SYSTEM">
        
        <id name="id">
            <column name="FIL_ID" />
            <generator class="native">
            </generator>
        </id>
		
		<property name="application" column="FIL_APPLICATION" type="string" not-null="true"/>
		
		<property name="year" column="FIL_YEAR" type="integer" not-null="true"/>
				
		<property name="nextDocNumber" column="FIL_NEXT_DOC" type="long" not-null="true"/>
			
    </class>
</hibernate-mapping>
