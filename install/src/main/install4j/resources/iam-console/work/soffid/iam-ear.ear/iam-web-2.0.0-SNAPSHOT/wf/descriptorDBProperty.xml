<?xml version="1.0" encoding="UTF-8"?>

<zkib-model>

	<datanode name="root">		
		<finder name="root" type="root">
			<ejb-finder jndi="java:local/ejb/BPMConfigLocal"
				method="findAll">
			</ejb-finder>
			<new-instance-bean className="es.caib.bpm.vo.ConfigParameterVO"/>
		</finder>
		<ejb-handler jndi="java:local/ejb/BPMConfigLocal">
			<update-method method="update">
				<parameter value="${instance}"/>
			</update-method>
			<insert-method method="create">
				<parameter value="${instance}" />
			</insert-method>
			<delete-method method="delete">
				<parameter value="${instance}" />
			</delete-method>
			
		</ejb-handler>
		
	</datanode>	
</zkib-model>
