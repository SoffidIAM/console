<?xml version="1.0" encoding="UTF-8" standalone="no"?><?taglib uri="/WEB-INF/tld/web/core.dsp.tld" prefix="c" ?>
<frame xmlns="http://www.zkoss.org/2005/zul" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:zk="http://www.zkoss.org/2005/zk" id="ui" title="${c:l('inbox.lblFiltroTareas2')}" use="es.caib.bpm.ui.inbox.RunningTasksHandler" width="100%" xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd"> 
	<div width="100%" sclass="frame-none">
	<toolbar>
			<toolbarbutton image="~./img/reload-petit16b.png" label="${c:l('inbox.lblRecargar')}" onClick="ui.refresh()" sclass="reloadtasks"/>
		</toolbar>
		<listbox fixedLayout="true" id="listadoTareas" mold="paging" pageSize="25" sclass="wf_listbox">
				<listhead>
					<listheader label="${c:l('inbox.lbldefinicionTarea')}" sort="auto" width="40%"/>
					<listheader label="${c:l('inbox.lblFecha')}" sort="auto" width="15%"/>
					<listheader label="${c:l('inbox.lblFechaPrevistaFin')}" sort="auto" width="15%"/>
					<listheader label="${c:l('inbox.lblActoresAsignados')}" sort="auto"/>
					<listheader label="${c:l('inbox.lblIdentificador')}" sort="auto" width="15%"/>
				</listhead>
		</listbox>
	</div>
</frame>