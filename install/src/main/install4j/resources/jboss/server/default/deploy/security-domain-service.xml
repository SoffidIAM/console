<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: security-domain-service.xml,v 1.1 2007-01-10 07:29:44 u07286 Exp $ -->

<server>

  <!-- The SSL domain setup -->
  <mbean code="org.jboss.security.plugins.JaasSecurityDomain"
      name="jboss.security:service=JaasSecurityDomain,domain=ssl">
    <constructor>
       <arg type="java.lang.String" value="ssl"/>
    </constructor>
    <attribute name="KeyStoreURL">test.keystore</attribute>
    <attribute name="KeyStorePass">contrasenya</attribute>
  </mbean>

</server>
