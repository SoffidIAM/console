<?xml version="1.0" encoding="UTF-8"?>

<language-addon>

	<addon-name>BPM-web</addon-name>
	 <version>
	  	<version-class>es.caib.bpm.ui.Version</version-class>
	  	<version-uid>1.0</version-uid>
	  </version>

	<language-name>xul/html</language-name>
	
	<component>
		<extends>treecell</extends>
		<component-name>apptreecell</component-name>
		<component-class>es.caib.bpm.ui.tree.ApplicationTreecell</component-class>
	</component>

	<component>
		<component-name>listbox</component-name>
		<extends>listbox</extends>
		<component-class>es.caib.zkib.component.DataListbox</component-class>
		<mold>
			<mold-name>label</mold-name>
			<mold-uri>~./zul/html/listbox-label.dsp</mold-uri>
		</mold>
	</component>	

	<component>
		<component-name>progress</component-name>
		<component-class>com.soffid.iam.web.component.ProgressImage</component-class>
		<extends>div</extends>
	</component>	

	<javascript-module name="attributesearchbox.attributesearchbox" version="1.0"/>
	<component>
		<component-name>attributesearchbox</component-name>
		<component-class>com.soffid.iam.web.component.AttributeSearchBox</component-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>~./zul/html/attributesearchbox.dsp</mold-uri>
		</mold>
	</component>	

	<component>
		<component-name>searchbox</component-name>
		<component-class>com.soffid.iam.web.component.SearchBox</component-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>~./zul/html/searchbox.dsp</mold-uri>
		</mold>
	</component>	
	
	<component>
		<extends>frame</extends>
		<component-name>frame</component-name>
		<component-class>com.soffid.iam.web.component.Frame</component-class>
	</component>

	
</language-addon>

