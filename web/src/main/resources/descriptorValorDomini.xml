		<finder name="valor" type="valor">
			<ejb-finder jndi="java:comp/env/ejb/DominiEJB"
				method="findValorsDominiByFiltre" if = "${queryEnabled}"> <!-- application:query -->
				<parameter value="${domini}"/>
				<parameter value="${codi}"/>
				<parameter value="${descripcio}"/>
				<parameter value="${usuari}"/>
			</ejb-finder>
			<new-instance-bean
				className="es.caib.seycon.ng.comu.ValorDomini">
			</new-instance-bean>			
		</finder>
	</datanode>	
	
	<datanode name="valor">
		<ejb-handler jndi="java:comp/env/ejb/DominiEJB">
			<insert-method method="create" returnBean="true">
				<parameter value="${instance}" />
			</insert-method>
			<delete-method method="delete">
				<parameter value="${instance}" />
			</delete-method>
		</ejb-handler>
	</datanode>
	
</zkib-model>
