<?xml version="1.0" encoding="UTF-8"?>

<zkib-model>

	<datanode name="root">
		<finder name="issue" type="issue">
			<ejb-finder jndi="java:/module/IssuesService-v2" method="findIssuesByJsonQueryAsync">
				<parameter value="${query}"/>
			</ejb-finder>
		</finder>
	</datanode>

	<datanode name="issue">
		<ejb-handler jndi="java:/module/IssueService-v2">
			<update-method method="update" returnBean="true">
				<parameter value="${instance}"/>
			</update-method>
		</ejb-handler>
	</datanode>

</zkib-model>
