<?xml version="1.0" encoding="UTF-8"?>

<zkib-model>

	<datanode name="root">
		<finder name="rule" type="rule">
			<ejb-finder jndi="java:/module/PamPolicyService-v2" method="findRuleByJsonQueryAsync">
				<parameter value="${textQuery}"/>
				<parameter value="${query}"/>
			</ejb-finder>
			<new-instance-bean
				className="com.soffid.iam.api.PamRule">
			</new-instance-bean>
		</finder>
	</datanode>

	<datanode name="rule">
		<ejb-handler jndi="java:/module/PamPolicyService-v2">
			<insert-method method="createRule" returnBean="true">
				<parameter value="${instance}"/>
			</insert-method>
			<delete-method method="deleteRule">
				<parameter value="${instance}"/>
			</delete-method>
			<update-method method="updateRule" returnBean="true">
				<parameter value="${instance}"/>
			</update-method>
		</ejb-handler>
	</datanode>

</zkib-model>
