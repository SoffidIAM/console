				void seguentCodiUsuari() {
					ds = esquema.getFellow("dades").getFellow("form").getDataSource();
					ctx = ds.getJXPathContext();
					String codiUsuari = ctx.getValue("@codi");
			
					javax.naming.Context context = new javax.naming.InitialContext();
